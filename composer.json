{
  "require": {
    "php": "^8.2",
    "php-ds/php-ds": "^1.4",
    "ext-readline": "*",
    "team-reflex/discord-php": "^7.3",
    "ext-curl": "*"
  },
  "autoload": {
    "psr-4": {
      "src\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.4"
  },
  "scripts": {
    "tests": "phpunit --testdox",
    "test-coverage": "phpunit --coverage-html build/coverage"
  }
}
